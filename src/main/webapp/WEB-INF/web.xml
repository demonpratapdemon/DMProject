  	<servlet-class>servlets.ContenAdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>admission</servlet-name>
  	<url-pattern>/admission</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdmissionServlet</servlet-name>
  	<url-pattern>/AdmissionServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ContenAdminServlet</servlet-name>
  	<url-pattern>/ContenAdminServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginDetailsServlet</servlet-name>
  	<url-pattern>/LoginDetailsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<url-pattern>/RegistrationServlet</url-pattern>
  </servlet-mapping>
 
  
</web-app>