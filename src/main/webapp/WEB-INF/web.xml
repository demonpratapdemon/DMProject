  <display-name>LoginDetailsServlet</display-name>
  <description></description>
  <servlet-class>servlets.LoginDetailsServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>RegistrationServlet</servlet-name>
  <display-name>RegistrationServlet</display-name>
